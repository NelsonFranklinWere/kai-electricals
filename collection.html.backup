        // Function to generate WhatsApp message
        function generateWhatsAppMessage(light) {
            return `Hi Spark_Lights254! 👋
I'm interested in this light:
🪄 ${light.name}
💰 Price: KES ${light.price.toLocaleString()}
You can view it here: ${light.imageUrl}

Please confirm availability.`;
        }

        // Function to render lights
        function renderLights(lightsArray) {
            const collectionGrid = document.getElementById('lights-collection');
            collectionGrid.innerHTML = '';
            lightsArray.forEach(light => {
                const lightCard = document.createElement('div');
                lightCard.className = 'product-card';
                lightCard.dataset.category = light.category;
                
                const whatsappMessage = generateWhatsAppMessage(light);
                const whatsappURL = `https://wa.me/254712827840?text=${encodeURIComponent(whatsappMessage)}`;
                
                lightCard.innerHTML = `
                    <div class="product-img" style="background-image: url('${light.image}')">
                        <div class="product-badge">${light.badge}</div>
                    </div>
                    <div class="product-info">
                        <h3>${light.name}</h3>
                        <p class="product-description">${light.description}</p>
                        <div class="product-price">KES ${light.price.toLocaleString()}</div>
                        <div class="product-actions">
                            <a href="${whatsappURL}" class="btn btn-whatsapp" target="_blank">
                                <i class="fab fa-whatsapp"></i> Order Now
                            </a>
                        </div>
                    </div>
                `;
                collectionGrid.appendChild(lightCard);
            });
        }

        // Filter functionality
        const filterButtons = document.querySelectorAll('.filter-btn');
        const searchInput = document.getElementById('search-input');

        filterButtons.forEach(button => {
            button.addEventListener('click', () => {
                // Remove active class from all buttons
                filterButtons.forEach(btn => btn.classList.remove('active'));
                // Add active class to clicked button
                button.classList.add('active');
                
                const filter = button.getAttribute('data-filter');
                filterLights(filter);
            });
        });

        // Search functionality
        searchInput.addEventListener('input', () => {
            const searchTerm = searchInput.value.toLowerCase();
            const activeFilter = document.querySelector('.filter-btn.active').getAttribute('data-filter');
            filterLights(activeFilter, searchTerm);
        });

        function filterLights(category, searchTerm = '') {
            let filteredLights = lights;
            
            // Filter by category
            if (category !== 'all') {
                filteredLights = filteredLights.filter(light => light.category === category);
            }
            
            // Filter by search term
            if (searchTerm) {
                filteredLights = filteredLights.filter(light => 
                    light.name.toLowerCase().includes(searchTerm) || 
                    light.description.toLowerCase().includes(searchTerm)
                );
            }
            
            renderLights(filteredLights);
        }

        // Initial render
        renderLights(lights);

        // Add scroll animation for header
        window.addEventListener('scroll', () => {
            const header = document.querySelector('header');
            if (window.scrollY > 100) {
                header.style.boxShadow = '0 5px 20px rgba(0, 0, 0, 0.1)';
            } else {
                header.style.boxShadow = '0 2px 15px rgba(0, 0, 0, 0.1)';
            }
        });
    </script>
</body>
</html>
